	agent node {
        label 'master'
        }
	
	stages {
		stage('Build') {
			steps {
				echo 'building'	
				echo 'hello worldassfsfs'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing..'
				echo 'double testing..'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying..'
			}
		}
		
	}
}
